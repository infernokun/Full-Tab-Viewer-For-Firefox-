{
    "manifest_version": 2,
    "name": "Full Tab Viewer",
    "version": "1.0",

    "description": "Displays all the tabs you have open with full titles.",

    "permissions": [
        "tabs"
    ],

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Full Tab Viewer",
        "default_popup": "view.html"
    }
}